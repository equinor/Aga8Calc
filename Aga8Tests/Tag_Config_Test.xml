<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <OpcUrl>opc.tcp://localhost:4840/freeopcua/server/</OpcUrl>
  <OpcUser></OpcUser>
  <OpcPassword></OpcPassword>
  <DefaultNamespaceURI>http://test.org/UA/Alarms/</DefaultNamespaceURI>
  <Interval>42.0</Interval>
  <ConfigList>
    <Config Name="Test Config 1">
      <Composition>
        <Component Name="Methane" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0.01" />
        <Component Name="Nitrogen" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="CarbonDioxide" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="Ethane" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="Propane" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="IsoButane" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="NormalButane" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="IsoPentane" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="NormalPentane" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="Hexane" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="Heptane" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="Octane" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="Nonane" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="Decane" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="Hydrogen" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="Oxygen" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="CarbonMonoxide" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="Water" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="HydrogenSulfide" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="Helium" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
        <Component Name="Argon" Tag="ns=2;s=1:AI1001?A" ScaleFactor="0" />
      </Composition>
      <PressureTemperatureList>
        <PressureTemperature>
          <PressureFunction MathFunction="Min">
            <Pressure Name="PF 1" Tag="ns=2;s=1:AI1001?Pressure" Unit="barg" />
            <Pressure Name="PF 2" Tag="ns=2;s=1:AI1002?Pressure" Unit="barg" />
          </PressureFunction>
          <TemperatureFunction MathFunction="Min">
            <Temperature Name="PF 1" Tag="ns=2;s=1:AI1001?Temperature" Unit="C" />
            <Temperature Name="PF 2" Tag="ns=2;s=1:AI1002?Temperature" Unit="C" />
          </TemperatureFunction>
          <Properties>
            <Property Tag="ns=2;s=1:AI1001?Result" Property="MolarConcentration" Type="double" />
          </Properties>
        </PressureTemperature>
      </PressureTemperatureList>
    </Config>
  </ConfigList>
</configuration>
